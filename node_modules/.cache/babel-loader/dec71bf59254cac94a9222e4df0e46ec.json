AlCA,qBAkCA;AACA;AACA,SAAA,IAAA;AACA,GArCA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,QAAA,EAAA,kBAAA,KAAA,EAAA;AACA,UAAA,CAAA,KAAA,EAAA,OAAA,MAAA;AACA,MAAA,KAAA,GAAA,KAAA,GAAA,CAAA;AACA,aAAA,MAAA,KAAA,CAAA,OAAA,CAAA,CAAA,CAAA;AACA;AALA,GAtCA;AA6CA,EAAA,OAAA,EAAA;AACA,IAAA,IADA,kBACA;AAAA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,iBAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,GAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAHA;AAIA,KANA;AAOA;AACA,IAAA,UARA,wBAQA;AACA,WAAA,YAAA,GAAA,KAAA;AACA,KAVA;AAWA;AACA,IAAA,cAZA,0BAYA,IAZA,EAYA;AACA,WAAA,OAAA,GAAA,IAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KAfA;AAgBA;AACA,IAAA,OAjBA,qBAiBA;AAAA;;AACA,UAAA,OAAA,GAAA,KAAA,OAAA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,YAAA,OAAA,CAAA,SAAA,IAAA,IAAA,CAAA,SAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,KAAA;AACA;AACA,OALA;AAMA,KAzBA;AA0BA;AACA,IAAA,QA3BA,oBA2BA,IA3BA,EA2BA,IA3BA,EA2BA;AACA,UAAA,IAAA,IAAA,KAAA,EAAA;AACA,QAAA,IAAA,CAAA,UAAA;AACA,OAFA,MAEA,IAAA,IAAA,IAAA,OAAA,EAAA;AACA,YAAA,IAAA,CAAA,UAAA,IAAA,CAAA,EAAA;AACA;AACA;;AACA,QAAA,IAAA,CAAA,UAAA;AACA,OALA,MAKA;AACA,QAAA,IAAA,CAAA,OAAA,GAAA,IAAA,CAAA,OAAA,GAAA,KAAA,GAAA,IAAA;AACA;AACA,KAtCA;AAuCA,IAAA,cAvCA,4BAuCA;AACA,UAAA,IAAA,GAAA,CAAA,KAAA,YAAA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,IAAA,CAAA,OAAA,GAAA,IAAA,GAAA,IAAA,GAAA,KAAA;AACA,OAFA;AAGA,KA5CA;AA6CA,IAAA,QA7CA,sBA6CA;AACA,UAAA,KAAA,YAAA,GAAA,CAAA,EAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA;AADA,SAAA;AAGA;AACA;AAnDA;AA7CA,CAAA","sourcesContent":["<template>\n  <div>\n    <nav-header></nav-header>\n    <div class=\"nav-breadcrumb-wrap\">\n      <div class=\"container\">\n        <nav class=\"nav-breadcrumb\">\n          <a href=\"/\">首页</a>\n          <span>购物车</span>\n        </nav>\n      </div>\n    </div>\n    <!-- 定义购物车页面图标 -->\n    <svg style=\"position: absolute; width: 0; height: 0; overflow: hidden;\" version=\"1.1\"\n         xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\">\n      <defs>\n        <symbol id=\"icon-ok\" viewBox=\"0 0 32 32\">\n          <title>ok</title>\n          <path class=\"path1\"\n                d=\"M31.020 0.438c-0.512-0.363-1.135-0.507-1.757-0.406s-1.166 0.435-1.529 0.937l-17.965 24.679-5.753-5.67c-0.445-0.438-1.035-0.679-1.664-0.679s-1.219 0.241-1.664 0.679c-0.917 0.904-0.917 2.375 0 3.279l7.712 7.6c0.438 0.432 1.045 0.681 1.665 0.681l0.195-0.008c0.688-0.057 1.314-0.406 1.717-0.959l19.582-26.9c0.754-1.038 0.512-2.488-0.538-3.233z\"></path>\n        </symbol>\n        <symbol id=\"icon-del\" viewBox=\"0 0 32 32\">\n          <title>delete</title>\n          <path class=\"path1\"\n                d=\"M11.355 4.129v-2.065h9.29v2.065h-9.29zM6.194 29.935v-23.742h19.613v23.742h-19.613zM30.968 4.129h-8.258v-3.097c0-0.569-0.463-1.032-1.032-1.032h-11.355c-0.569 0-1.032 0.463-1.032 1.032v3.097h-8.258c-0.569 0-1.032 0.463-1.032 1.032s0.463 1.032 1.032 1.032h3.097v24.774c0 0.569 0.463 1.032 1.032 1.032h21.677c0.569 0 1.032-0.463 1.032-1.032v-24.774h3.097c0.569 0 1.032-0.463 1.032-1.032s-0.463-1.032-1.032-1.032v0z\"></path>\n          <path class=\"path2\"\n                d=\"M10.323 9.806c-0.569 0-1.032 0.463-1.032 1.032v14.452c0 0.569 0.463 1.032 1.032 1.032s1.032-0.463 1.032-1.032v-14.452c0-0.569-0.463-1.032-1.032-1.032z\"></path>\n          <path class=\"path3\"\n                d=\"M16 9.806c-0.569 0-1.032 0.463-1.032 1.032v14.452c0 0.569 0.463 1.032 1.032 1.032s1.032-0.463 1.032-1.032v-14.452c0-0.569-0.463-1.032-1.032-1.032z\"></path>\n          <path class=\"path4\"\n                d=\"M21.677 9.806c-0.569 0-1.032 0.463-1.032 1.032v14.452c0 0.569 0.463 1.032 1.032 1.032s1.032-0.463 1.032-1.032v-14.452c0-0.569-0.463-1.032-1.032-1.032z\"></path>\n        </symbol>\n      </defs>\n    </svg>\n    <div class=\"container\">\n      <div class=\"cart\">\n        <div class=\"page-title-normal\">\n          <h2 class=\"page-title-h2\"><span>我的购物车</span></h2>\n        </div>\n        <div class=\"item-list-wrap\">\n          <div class=\"cart-item\">\n            <div class=\"cart-item-head\">\n              <ul>\n                <li>商品信息</li>\n                <li>商品金额</li>\n                <li>商品数量</li>\n                <li>总金额</li>\n                <li>编辑</li>\n              </ul>\n            </div>\n            <ul class=\"cart-item-list\">\n              <li v-for=\"item in cartList\"  :key=\"item.one\">\n                <div class=\"cart-tab-1\">\n                  <div class=\"cart-item-check\">\n                    <a href=\"javascipt:;\" class=\"checkbox-btn item-check-btn\" v-bind:class=\"{'check':item.checked}\" @click=\"editCart('checked',item)\">\n                      <svg class=\"icon icon-ok\">\n                        <use xlink:href=\"#icon-ok\"></use>\n                      </svg>\n                    </a>\n                  </div>\n                  <div class=\"cart-item-pic\">\n                    <img v-bind:src=\"'/imgs/'+item.productImage\" v-bind:alt=\"item.productName\">\n                  </div>\n                  <div class=\"cart-item-title\">\n                    <div class=\"item-name\">{{item.productName}}</div>\n                  </div>\n                </div>\n                <div class=\"cart-tab-2\">\n                  <div class=\"item-price\">{{item.salePrice | currency}}</div>\n                </div>\n                <div class=\"cart-tab-3\">\n                  <div class=\"item-quantity\">\n                    <div class=\"select-self select-self-open\">\n                      <div class=\"select-self-area\">\n                        <a class=\"input-sub\" @click=\"editCart('minus',item)\">-</a>\n                        <span class=\"select-ipt\">{{item.productNum}}</span>\n                        <a class=\"input-add\" @click=\"editCart('add',item)\">+</a>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n                <div class=\"cart-tab-4\">\n                  <div class=\"item-price-total\">{{(item.productNum*item.productPrice) | currency }}</div>\n                </div>\n                <div class=\"cart-tab-5\">\n                  <div class=\"cart-item-opration\">\n                    <a href=\"javascript:;\" class=\"item-edit-btn\" @click=\"delCartConfirm(item)\">\n                      <svg class=\"icon icon-del\">\n                        <use xlink:href=\"#icon-del\"></use>\n                      </svg>\n                    </a>\n                  </div>\n                </div>\n              </li>\n            </ul>\n          </div>\n        </div>\n        <div class=\"cart-foot-wrap\">\n          <div class=\"cart-foot-inner\">\n            <div class=\"cart-foot-l\">\n              <div class=\"item-all-check\">\n                <a href=\"javascipt:;\" @click=\"toggleCheckAll\">\n                  <span class=\"checkbox-btn item-check-btn\" v-bind:class=\"{'check':checkAllFlag}\">\n                      <svg class=\"icon icon-ok\"><use xlink:href=\"#icon-ok\"/></svg>\n                  </span>\n                  <span>全选</span>\n                </a>\n              </div>\n            </div>\n            <div class=\"cart-foot-r\">\n              <div class=\"item-total\">\n                总价: <span class=\"total-price\">{{totalPrice | currency}}</span>\n              </div>\n              <div class=\"btn-wrap\">\n                <a class=\"btn btn--red\" v-bind:class=\"{'btn--dis':checkedCount==0}\" @click=\"checkOut\">结算</a>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <Modal :mdShow=\"modalConfirm\" @close=\"closeModal\">\n      <p slot=\"message\">你确认要删除此条数据吗?</p>\n      <a slot=\"btnGroup\" class=\"btn btn--m\" href=\"javascript:;\" @click=\"delCart\">确认</a>\n      <a slot=\"btnGroup\" class=\"btn btn--m btn--red\" href=\"javascript:;\" @click=\"modalConfirm = false\">关闭</a>\n    </Modal>\n    <nav-footer></nav-footer>\n  </div>\n</template>\n<script>\n  import NavHeader from './../components/Header'\n  import NavFooter from './../components/Footer'\n  import Modal from './../components/Modal'\n  export default{\n      data(){\n          return{\n            cartList:[],\n            modalConfirm:false\n          }\n      },\n      components:{\n        NavHeader,\n        NavFooter,\n        Modal\n      },\n      computed:{\n        // 当选中的数量和购物车商品数量一样多的时候，表示全选，every判断是否全部满足\n        checkAllFlag(){\n          return this.cartList.every((item)=>{\n            return item.checked;\n          });\n        },\n        // 判断当前有没有选中的商品,some判断是否有满足的元素\n        checkedCount(){\n          return this.cartList.some((item)=>item.checked)\n        },\n        // 计算总金额\n        totalPrice(){\n          let money = 0;\n          this.cartList.forEach((item)=>{\n            if(item.checked){\n              money += item.productPrice*item.productNum;\n            }\n          })\n          return money;\n        }\n      },\n      mounted(){\n        // 初始化购物车列表\n        this.init();\n      },\n      filters: {\n        currency: function (value) {\n          if (!value) return '0.00'\n          value = value*1;\n          return '￥' + value.toFixed(2);\n        }\n      },\n      methods:{\n        init(){\n          this.axios.get(\"/mock/cart.json\").then((response)=>{\n              let res = response.data;\n              this.cartList = res.data;\n          });\n        },\n        // 关闭弹框\n        closeModal(){\n          this.modalConfirm = false;\n        },\n        // 删除确认\n        delCartConfirm(item){\n            this.delItem = item;\n            this.modalConfirm = true;\n        },\n        // 删除\n        delCart(){\n          let delItem = this.delItem;\n          this.cartList.forEach((item,index)=>{\n            if(delItem.productId == item.productId){\n              this.cartList.splice(index,1);\n              this.modalConfirm = false;\n            }\n          })\n        },\n        // 修改商品对象（数量和选中）\n        editCart(flag,item){\n            if(flag=='add'){\n              item.productNum++;\n            }else if(flag=='minus'){\n              if(item.productNum<=1){\n                return;\n              }\n              item.productNum--;\n            }else{\n              item.checked = item.checked?false:true;\n            }\n        },\n        toggleCheckAll(){\n            var flag = !this.checkAllFlag;\n            this.cartList.forEach((item)=>{\n              item.checked = flag?true:false;\n            })\n        },\n        checkOut(){\n            if(this.checkedCount>0){\n                this.$router.push({\n                    path:\"/address\"\n                });\n            }\n        }\n      }\n  }\n</script>\n"],"sourceRoot":"src/pages"}]}